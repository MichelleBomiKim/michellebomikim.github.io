install.packages("haven") library("haven") TEDS_2016\<-read_stata("<https://github.com/datageneration/home/blob/master/DataProgramming/data/TEDS_2016.dta?raw=true>") library(regplot) TEDS_2016$Tondu <-as.numeric(TEDS_2016$Tondu,labels=c("Unification now", "Status quo, unif. in future", "Status quo, decide later", "Status quo forever", "Status quo, indep. in future", "Independence now", "No response")) df1 \<- TEDS_2016$$,c("Tondu", "age","edu","income")$$ reg \<- lm (Tondu\~ age+ edu + income, data=df1) summary(reg) regplot(reg)
